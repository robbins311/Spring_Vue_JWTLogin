AAA,CAAA;AACA,aAAA,KAAA,GAAA,KAAA;AACA,QAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA;AAEA;AAlCA;AAPA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div id=\"authModal\">\r\n      <div class=\"black-bg\" v-if=\"modal == true\">\r\n        <div class=\"white-bg\" @click=\"modal=false\">\r\n          <img src=\"../../assets/logo.png\" alt=\"\">\r\n          <h1>인증된 사용자 입니다.</h1>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <button @click=\"onAuthClicked\" >인증하기</button>\r\n    <button @click=\"logout\" v-if=\"leaveBtn == false\" >로그아웃</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Me\",\r\n  data() {\r\n    return {\r\n      modal: false,\r\n      leaveBtn: false\r\n    }\r\n  },  methods: {\r\n      onAuthClicked() {\r\n        const token = localStorage.getItem('token')\r\n        let config = {\r\n          headers: {\r\n            \"Authorization\": `Bearer ${token}`,\r\n          }\r\n        }\r\n        this.$axios\r\n        .get('http://localhost:8081/member/me', config)\r\n        .then(res=>{\r\n          alert(\"Authorization User\")\r\n          this.modal = true\r\n          console.log(res.data)\r\n        })\r\n        .catch(error => {\r\n          //console.log(key)\r\n          alert(\"로그인 하세요!\")\r\n          this.modal = false\r\n          console.log(token)\r\n          console.log(error)\r\n        })\r\n      },\r\n      logout() {\r\n        if (localStorage.getItem('token') == undefined ) {\r\n          this.leaveBtn == true;\r\n          alert(\"이미 로그아웃 되었습니다.\")\r\n          return null;\r\n        } else {\r\n          alert(\"로그아웃 되었습니다.\")\r\n          this.modal = false\r\n          localStorage.removeItem('token');\r\n        }\r\n\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\n#authModal {\r\n  width: 500px;\r\n  height: 385px;\r\n}\r\n.black-bg {\r\n  width: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  position: fixed;\r\n  padding: 20px;\r\n}\r\n.white-bg {\r\n  width: 100%;\r\n  background: white;\r\n  border-radius: 8px;\r\n  padding: 20px;\r\n}\r\n</style>"],"sourceRoot":"src/pages/member"}]}